{
  "name": "mithril-transition-injector",
  "version": "2.1.0",
  "description": "The most easy and awesome way to add mount and unmount transitions to your components with CSS",
  "repository": {
    "type": "git",
    "url": "https://github.com/CapnMarius/MithrilTransitionInjector.git"
  },
  "main": "dist/index.js",
